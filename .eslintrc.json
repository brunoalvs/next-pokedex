{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "next/core-web-vitals",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime"
  ],
  "plugins": ["react"],
  "rules": {
    "space-before-function-paren": ["error", "always"],
    "array-callback-return": "off",
    "indent": ["error", 2]
  }
}
